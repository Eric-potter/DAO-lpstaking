{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "./build.sh",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true,
			},
			"label": ". build.sh"
		},
		{
			"type": "shell",
			"command": "cargo +nightly test",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true,
			},
			"label": "cargo +nightly test"
		}
	]
}